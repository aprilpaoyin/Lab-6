package com.lab1.test;

public class Vehicle 
{
	//attributes
	String owner;
	int regisNum;
	int maxSpeed;
	String colour;
	boolean auto;
	int numWheels;
	
	//constructors
	Vehicle(String owner)
	{
		this.owner = owner;
	}//end 1st constructor
	
	Vehicle(int regisNum, int maxSpeed, String colour, boolean auto, int numWheels)
	{
		this.regisNum = regisNum;
		this.maxSpeed = maxSpeed;
		this.colour = colour;
		this.auto = auto;
		this.numWheels = numWheels;
		
		System.out.println(owner + " " + this.regisNum + " " + this.maxSpeed + " " + this.colour + " " + this.auto + " " + this.numWheels);
	}//end 2nd constructor
}//end Vehicle
